{
  "name": "mmf-blog-vue2-ssr",
  "version": "2.0.0",
  "description": "A Vue.js project wuth vue 2.0, vue-router and vuex for server side rendering.",
  "keywords": [
    "vue",
    "vuex",
    "vue-router",
    "webpack",
    "starter",
    "server-side",
    "boilerplate"
  ],
  "author": "lincenying <lincenying@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "npm run static && node ./server",
    "start": "cross-env NODE_ENV=production node ./server",
    "build": "npm run static && npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.client.config.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.config.js --progress --hide-modules",
    "static": "node ./build/copy",
    "lint:client": "eslint --ext .jsx,.js,.vue src/ --fix",
    "lint:server": "eslint --ext .js server/ --fix",
    "lint": "npm run lint:client && npm run lint:server"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "fastclick": "^1.0.6",
    "highlight.js": "^9.12.0",
    "iview": "^2.13.0",
    "jquery": "^3.2.1",
    "js-cookie": "^2.2.0",
    "marked": "^0.3.19",
    "md5": "^2.2.1",
    "moment": "^2.22.0",
    "nprogress": "^0.2.0",
    "store2": "^2.7.0",
    "toastr": "^2.1.2",
    "vue": "^2.5.16",
    "vue-meta": "^1.5.0",
    "vue-router": "^3.0.1",
    "vue-server-renderer": "^2.5.16",
    "vue-ssr-html-stream": "^2.2.0",
    "vue-template-compiler": "^2.5.16",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^8.2.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-runtime": "^6.26.0",
    "body-parser": "^1.18.2",
    "browserslist": "^3.2.4",
    "compression": "^1.7.2",
    "connect-multiparty": "^2.1.0",
