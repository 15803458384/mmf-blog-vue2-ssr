{
  "name": "vue2-ssr-template",
  "version": "1.0.0",
  "description": "A Vue.js project wuth vue 2.0, vue-router and vuex for server side rendering.",
  "keywords": [
    "vue",
    "vuex",
    "vue-router",
    "webpack",
    "starter",
    "server-side",
    "boilerplate"
  ],
  "author": "lincenying <lincenying@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "node ./server",
    "start": "NODE_ENV=production node ./server",
    "build": "npm run build:client && npm run build:server",
    "build:client": "NODE_ENV=production webpack --config build/webpack.client.config.js --progress --hide-modules",
    "build:server": "NODE_ENV=production webpack --config build/webpack.server.config.js --progress --hide-modules"
  },
  "dependencies": {
    "jquery": "^3.1.0",
    "leancloud-storage": "^1.3.3",
    "nprogress": "^0.2.0",
    "store2": "^2.3.2",
    "toastr": "^2.1.2",
    "vue": "^2.0.0-rc.3",
    "vue-router": "^2.0.0-rc.3",
    "vue-server-renderer": "^2.0.0-rc.3",
    "vue-template-compiler": "^2.0.0-rc.3",
    "vuex": "^2.0.0-rc.5",
    "vuex-router-sync": "^3.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-stage-2": "^6.13.0",
    "cookie-parser": "^1.4.3",
    "css-loader": "^0.23.1",
    "ejs": "^2.5.1",
    "eslint": "^3.3.1",
    "eslint-config-standard": "^6.0.0-beta.2",
    "eslint-friendly-formatter": "^2.0.6",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-html": "^1.5.2",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-sort-class-members": "^1.0.1",
    "eslint-plugin-standard": "^2.0.0",
    "eslint-plugin-vue": "^0.1.1",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.3",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "http-proxy-middleware": "^0.17.1",
    "json-loader": "^0.5.4",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "lru-cache": "^4.0.1",
    "postcss-loader": "^0.10.1",
    "serialize-javascript": "^1.3.0",
    "serve-favicon": "^2.3.0",
    "style-loader": "^0.13.1",
    "vue-loader": "^9.4.0",
    "webpack": "^2.1.0-beta.20",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.2",
    "webpack-merge": "^0.14.1"
  }
}
