{
  "name": "mmf-blog-vue2-ssr",
  "version": "2.0.0",
  "description": "A Vue.js project wuth vue 2.0, vue-router and vuex for server side rendering.",
  "keywords": [
    "vue",
    "vuex",
    "vue-router",
    "webpack",
    "starter",
    "server-side",
    "boilerplate"
  ],
  "author": "lincenying <lincenying@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "yarn static && node ./server",
    "start": "cross-env NODE_ENV=production node ./server",
    "build": "yarn static && yarn build:client && yarn build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.client.config.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.config.js --progress --hide-modules",
    "static": "node ./build/copy",
    "lint:client": "eslint --ext .jsx,.js,.vue src/ --fix",
    "lint:server": "eslint --ext .js server/ --fix",
    "lint": "yarn lint:client && yarn lint:server"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "fastclick": "^1.0.6",
    "highlight.js": "^9.15.6",
    "jquery": "^3.2.1",
    "js-cookie": "^2.2.0",
    "marked": "^0.6.1",
    "md5": "^2.2.1",
    "moment": "^2.24.0",
    "nprogress": "^0.2.0",
    "store2": "^2.7.1",
    "toastr": "^2.1.2",
    "vue": "^2.6.8",
    "vue-meta": "^1.5.8",
    "vue-router": "^3.0.2",
    "vue-server-renderer": "^2.6.8",
    "vue-ssr-html-stream": "^2.2.0",
    "vue-template-compiler": "^2.6.8",
    "vuex": "^3.1.0",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "7.3.4",
    "@babel/plugin-proposal-decorators": "7.3.0",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.4",
    "@babel/runtime": "^7.3.4",
    "autoprefixer": "^9.4.10",
    "babel-loader": "^8.0.5",
    "body-parser": "^1.18.3",
    "browserslist": "^4.4.2",
    "cache-loader": "^2.0.1",
    "compression": "^1.7.3",
    "connect-multiparty": "^2.2.0",
    "cookie-parser": "^1.4.4",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "ejs": "^2.6.1",
    "eslint-config-lcy-vue": "^3.0.5",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.16.4",
    "file-loader": "^3.0.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "json-loader": "^0.5.7",
    "jsonwebtoken": "^8.5.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "lint-staged": "^8.1.5",
    "lru-cache": "^5.1.1",
    "mini-css-extract-plugin": "0.5.0",
    "mongoose": "^5.4.19",
    "morgan": "^1.9.1",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.16.4",
    "raven-js": "^3.27.0",
    "serve-favicon": "^2.5.0",
    "shelljs": "^0.8.3",
    "style-loader": "^0.23.1",
    "sw-precache-webpack-plugin": "^0.11.5",
    "thread-loader": "^2.1.2",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "url-loader": "^1.1.2",
    "vue-content-loader": "^0.2.1",
    "vue-loader": "^15.7.0",
    "vue-ssr-webpack-plugin": "^3.0.0",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-middleware": "^3.6.1",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2",
    "webpackbar": "^3.1.5"
  },
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 5.0.0"
  },
  "browserslist": [
    "> 0.1%",
    "last 3 versions",
    "not ie <= 8",
    "iOS > 8"
  ],
  "lint-staged": {
    "*.jsx": [
      "eslint --fix",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.vue": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
